{
    "name": "@atlanhq/i18n",
    "version": "1.0.3",
    "description": "Internationalization package for Atlan's frontend applications",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "src/locales"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./locales/*": "./src/locales/*",
        "./en": {
            "types": "./dist/en/index.d.ts",
            "require": "./dist/en/index.js",
            "import": "./dist/en/index.mjs"
        },
        "./fr": {
            "types": "./dist/fr/index.d.ts",
            "require": "./dist/fr/index.js",
            "import": "./dist/fr/index.mjs"
        },
        "./jp": {
            "types": "./dist/jp/index.d.ts",
            "require": "./dist/jp/index.js",
            "import": "./dist/jp/index.mjs"
        },
        "./pt": {
            "types": "./dist/pt/index.d.ts",
            "require": "./dist/pt/index.js",
            "import": "./dist/pt/index.mjs"
        },
        "./de": {
            "types": "./dist/de/index.d.ts",
            "require": "./dist/de/index.js",
            "import": "./dist/de/index.mjs"
        },
        "./dprod": {
            "types": "./dist/dprod/index.d.ts",
            "require": "./dist/dprod/index.js",
            "import": "./dist/dprod/index.mjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean false && node scripts/generate-locales.mjs",
        "prepare": "pnpm build",
        "test": "jest",
        "lint": "eslint .",
        "translate": "node scripts/translationScript.mjs",
        "format": "prettier --write \"src/**/*.{ts,json}\"",
        "prepublishOnly": "pnpm build",
        "release:local": "node scripts/local-release.mjs"
    },
    "peerDependencies": {
        "vue": "^3.0.0",
        "vue-i18n": "^11.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/atlanhq/i18n.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "restricted"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "translation",
        "atlan",
        "vue",
        "vue-i18n"
    ],
    "author": "Atlan",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/atlanhq/i18n/issues"
    },
    "homepage": "https://github.com/atlanhq/i18n#readme",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vue/test-utils": "^2.0.0",
        "eslint": "^8.0.0",
        "glob": "^11.0.2",
        "jest": "^29.0.0",
        "prettier": "^3.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vue": "^3.0.0",
        "vue-i18n": "^9.14.5",
        "vue-i18n-extract": "^2.0.7",
        "semantic-release": "^23.0.0",
        "@semantic-release/commit-analyzer": "^11.1.0",
        "@semantic-release/release-notes-generator": "^12.1.0",
        "@semantic-release/npm": "^11.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.5"
    }
}
